<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="content" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewSujetClientController">
   <children>
      <TableView fx:id="tablestage" layoutX="67.0" layoutY="58.0" prefHeight="259.0" prefWidth="436.0">
        <columns>
          <TableColumn fx:id="sujet" prefWidth="139.0" text="Sujet" />
          <TableColumn fx:id="description" prefWidth="163.0" text="description" />
            <TableColumn fx:id="branche" prefWidth="133.0" text="Branche" />
        </columns>
      </TableView>
      <Button fx:id="postuler" layoutX="441.0" layoutY="337.0" mnemonicParsing="false" onAction="#postuler" prefHeight="25.0" prefWidth="87.0" style="-fx-background-color: #6747cd;" text="postuler" />
      <ImageView fitHeight="54.0" fitWidth="106.0">
         <image>
            <Image url="@../images/img.png" />
         </image>
      </ImageView>
      <TextField fx:id="search" layoutX="385.0" layoutY="15.0" onKeyTyped="#searchSujet" prefHeight="25.0" prefWidth="208.0" promptText="taper le sujet que vous souhaitez" />
   </children>
</AnchorPane>
